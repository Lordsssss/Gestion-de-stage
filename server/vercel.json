{
    "version": 2,
    "functions": {
      "api/**/*.js": {
        "runtime": "@vercel/node@1.3.0"
      }
    },
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.js"
      }
    ],
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        },
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        }  
    ]
  }
